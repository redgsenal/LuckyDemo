package com.senal.lucky;

import java.util.Random;

public class Pick {
	private int value;

	public Pick(double minValue, double maxValue) {
		value = getRandomIntegerBetweenRange(minValue, maxValue);
	}

	public int getValue() {
		return value;
	}

	private int getRandomIntegerBetweenRange(double min, double max) {
	    return (new Random()).nextInt((max - min) + 1) + min;
	    
	}
}
